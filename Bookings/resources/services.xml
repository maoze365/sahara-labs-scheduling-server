<?xml version="1.0" encoding="UTF-8"?>
<serviceGroup>
    <service name="Bookings">
        <messageReceivers>
            <messageReceiver mep="http://www.w3.org/ns/wsdl/in-out" class="au.edu.uts.eng.remotelabs.schedserver.bookings.intf.BookingsMessageReceiverInOut"/>
        </messageReceivers>
        <parameter name="ServiceClass">au.edu.uts.eng.remotelabs.schedserver.bookings.intf.BookingsSkeleton</parameter>
        <parameter name="useOriginalwsdl">true</parameter>
        <parameter name="modifyUserWSDLPortAddress">true</parameter>
        <operation name="deleteBookings" mep="http://www.w3.org/ns/wsdl/in-out" namespace="http://remotelabs.eng.uts.edu.au/schedserver/bookings">
            <actionMapping>http://remotelabs.eng.uts.edu.au/schedserver/bookings/deleteBookings</actionMapping>
            <outputActionMapping>http://remotelabs.eng.uts.edu.au/schedserver/bookings/Bookings/deleteBookingsResponse</outputActionMapping>
        </operation>
        <operation name="findFreeBookings" mep="http://www.w3.org/ns/wsdl/in-out" namespace="http://remotelabs.eng.uts.edu.au/schedserver/bookings">
            <actionMapping>http://remotelabs.eng.uts.edu.au/schedserver/bookings/findFreeBookings</actionMapping>
            <outputActionMapping>http://remotelabs.eng.uts.edu.au/schedserver/bookings/Bookings/findFreeBookingsResponse</outputActionMapping>
        </operation>
        <operation name="getBooking" mep="http://www.w3.org/ns/wsdl/in-out" namespace="http://remotelabs.eng.uts.edu.au/schedserver/bookings">
            <actionMapping>http://remotelabs.eng.uts.edu.au/schedserver/bookings/getBooking</actionMapping>
            <outputActionMapping>http://remotelabs.eng.uts.edu.au/schedserver/bookings/Bookings/getBookingResponse</outputActionMapping>
        </operation>
        <operation name="createBooking" mep="http://www.w3.org/ns/wsdl/in-out" namespace="http://remotelabs.eng.uts.edu.au/schedserver/bookings">
            <actionMapping>http://remotelabs.eng.uts.edu.au/schedserver/bookings/createBooking</actionMapping>
            <outputActionMapping>http://remotelabs.eng.uts.edu.au/schedserver/bookings/Bookings/createBookingResponse</outputActionMapping>
        </operation>
        <operation name="getBookings" mep="http://www.w3.org/ns/wsdl/in-out" namespace="http://remotelabs.eng.uts.edu.au/schedserver/bookings">
            <actionMapping>http://remotelabs.eng.uts.edu.au/schedserver/bookings/NewOperation</actionMapping>
            <outputActionMapping>http://remotelabs.eng.uts.edu.au/schedserver/bookings/Bookings/getBookingsResponse</outputActionMapping>
        </operation>
    </service>
</serviceGroup>
