  		<wsdl:output>
  			<soap:body use="literal" />
  		</wsdl:output>
  	</wsdl:operation>
  	<wsdl:operation name="getBooking">
  		<soap:operation
  			soapAction="http://remotelabs.eng.uts.edu.au/schedserver/bookings/getBooking" />
  		<wsdl:input>
  			<soap:body use="literal" />
  		</wsdl:input>
  		<wsdl:output>
